CONFIG_IMX_CONFIG="board/freescale/imxrt1050-evk/imximage-nor.cfg"
CONFIG_USE_BOOTCOMMAND=y
CONFIG_BOOTCOMMAND="setenv bootargs console=ttyLP0 root=/dev/mtdblock0 rw earlycon rootwait;bootm 0x6008D800 - 0x6008C000"
CONFIG_SPL_XIP_SUPPORT=y
CONFIG_SPL_OS_BOOT=y
CONFIG_SPL_PAYLOAD_ARGS_ADDR=0x6008C000
CONFIG_SYS_OS_BASE=0x6008D800
